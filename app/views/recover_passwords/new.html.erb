<h1>Recover Password</h1>

<%= form_for(@recover_password) do |f| %>
	<%= render 'shared/error_messages', :object => f.object %>

  	<div class="field">
		<%= f.label :email %><br />
		<%= f.text_field :email %>
	</div>

  <div class="actions">
    <%= f.submit "Send Recover Password Email" %>
  </div>
<% end %>
