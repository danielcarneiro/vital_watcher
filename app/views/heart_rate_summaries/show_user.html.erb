<%= render 'shared/user_header' %>

<h2>Heart Rates</h2><br />

<% if @heart_rate_summaries.empty? %>
	There's no entries for this user<br />
<% else %>
	<table class="heart_rate_summaries" summary="Heart Rate Summaries">
		<thead>
			<tr>
				<th>Value</th>
				<th>Occurrences</th>
				<th>Percent</th>
			</tr>
		</thead>
		<%= render @heart_rate_summaries %>
	</table>
<% end %>


